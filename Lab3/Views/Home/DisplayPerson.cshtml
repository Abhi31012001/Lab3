@model Lab3.Models.Person
@{ Layout = "_Layout";
    ViewData["Title"] = "Display Person"; }

<h3>Display Form Data</h3>
<table>
    <tr>
        <td>
            First Name
        </td>
        <td>
            @Model.FirstName
        </td>
    </tr>
    <tr>
        <td>
            Last Name
        </td>
        <td>
            @Model.LastName
        </td>
    </tr>
    <tr>
        <td>
            Age
        </td>
        <td>
            @Model.Age
        </td>
    </tr>
    <tr>
        <td>
            Email Address:
        </td>
        <td>
            @Model.EmailAddress
        </td>
    </tr>
    <tr>
        <td>
            Date of Birth:
        </td>
        <td>
            @Model.Dob
        </td>
    </tr>
    <tr>
        <td>
            Description:
        </td>
        <td>
            @Model.Description
        </td>
    </tr>
</table>
